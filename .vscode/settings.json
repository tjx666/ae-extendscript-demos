{
    "files.exclude": {
        "**/.git": true
    },
    "vsicons.associations.files": [{ "extensions": ["jsx"], "icon": "js", "format": "svg" }],
    "files.associations": {
        "*.jsx": "javascript",
    },
    "typescript.tsdk": "node_modules/typescript/lib",
    "[javascript][typescript][typescriptreact][json][jsonc][html][css][less][markdown][xml][yaml][svg]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    }
}
